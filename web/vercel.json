{
  "headers": [
    {
      "source": "/api/getgroup/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=400, stale-while-revalidate=899"
        }
      ]
    },
    {
      "source": "/api/covid/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=1800, stale-while-revalidate=3600"
        }
      ]
    }
  ],
  "functions": {
    "api/getgroup.ts": {
      "includeFiles": "./api/**"
    },
    "api/user.ts": {
      "includeFiles": "./node_modules/bcrypt/**"
    }
  }
}
